                                <div class="input-items default">
                                    <ul
                                        class="exploitation_charge"
                                        data-index="{{ form.exploitation_charge|length > 0 ? form.exploitation_charge|last.vars.name + 1 : 0 }}"
                                        data-prototype="{{ form_widget(form.exploitation_charge.vars.prototype)|e('html_attr') }}"
