<section class="signin-area signin-one">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-6">
                <div class="mb-5">
                    <h1 class="text-center">
                        {{ title }}
                    </h1>
                </div>
                {{
                    form_start(form, {
                        attr: {
                            novalidate: "novalidate",
                            id: "productForm"
                        }
                    })
                }}
                <div class="signin-form form-style-two rounded-buttons">
                    <div class="row">
                        <div class="col-md-12">
                            {% if form_errors(form.year) %}
                            <div
                                class="form-input alert alert-danger"
                                role="alert"
                            >
                                {{ form_errors(form.year) }}
                            </div>
                            {% endif %}
                            <div class="form-input">
                                <label>Ann√©e</label>
                                <div class="input-items default">
                                    {{
                                        form_widget(form.year, {
                                            attr: {
                                                placeholder: ""
                                            }
                                        })
                                    }}
                                    <i class="fa fa-edit"></i>
                                </div>
                            </div>
                            <!-- form input -->
                        </div>
                        <div class="col-md-12">
                            {% if form_errors(form.month) %}
                            <div
                                class="form-input alert alert-danger"
                                role="alert"
                            >
                                {{ form_errors(form.month) }}
                            </div>
                            {% endif %}
                            <div class="form-input">
                                <label>Mois</label>
                                <div class="input-items default">
                                    {{
                                        form_widget(form.month, {
                                            attr: {
                                                placeholder: "Month"
                                            }
                                        })
                                    }}
                                    <i class="fa fa-edit"></i>
                                </div>
                            </div>
                            <!-- form input -->
                        </div>

