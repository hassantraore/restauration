                                        href="#"
                                        onclick="event.preventDefault()"
                                        style="margin-bottom: 1rem"
                                        class="btn btn-primary btn-sm add_item_link"
                                        data-collection-holder-class="exploitation_charge"
                                    >
                                        Add an exploitation charge
                                    </a>
                                </div>
                            </div>
                            <!-- form input -->
                        </div>
